% Matrix multiplication and structure usage
A = [1, 2; 3, 4];
B = [5, 6; 7, 8];
C = A * B;
% Store in a structure
resultStruct.product = C;
resultStruct.description = 'Result of matrix multiplication';
disp('Matrix Multiplication Result:');
disp(resultStruct.product);
disp('Description:');
disp(resultStruct.description);
